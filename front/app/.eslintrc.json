{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": "off",
        "max-len": ["error", { "code": 80, "ignoreTemplateLiterals": true }]
    },
    "ignorePatterns": ["node_modules/", "dist/"]
}